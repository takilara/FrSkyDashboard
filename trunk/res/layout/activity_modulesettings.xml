<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout android:id="@+id/linearLayout1" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearLayout2" android:layout_width="match_parent" android:layout_height="wrap_content" android:gravity="center_horizontal">
            <TextView android:text="@string/FrSkySettings_RSSI" android:background="#FF909090" 
            android:textColor="#FFFFFFFF" 
            android:id="@+id/textView2" android:layout_height="wrap_content" android:layout_gravity="center" android:layout_width="fill_parent" android:gravity="center" android:textStyle="bold"></TextView>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearLayout3" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:text="@string/FrSkySettings_Alarm_1" android:layout_gravity="left" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1.1"></TextView>
            <Spinner android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/FrSkySettings_spinner_level" android:layout_gravity="right" android:layout_weight="0.5"></Spinner>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearLayout4" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:text="@string/FrSkySettings_Value" android:layout_width="wrap_content" android:id="@+id/textView3" android:layout_height="wrap_content" android:layout_gravity="left" android:layout_weight="1"></TextView>
            <Spinner android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/FrSkySettings_spinner_relative" android:layout_gravity="left" android:layout_weight="1"></Spinner>
            <TextView android:text="@string/FrSkySettings_Than" android:layout_width="wrap_content" android:id="@+id/textView4" android:layout_height="wrap_content" android:layout_gravity="right" android:layout_weight="1"></TextView>
            <Spinner android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/FrSkySettings_spinner_value" android:layout_gravity="right" android:layout_weight="1"></Spinner>
        </LinearLayout>
        <Button android:id="@+id/FrSkySettings_send" android:text="@string/FrSkySettings_Send" android:layout_height="wrap_content" android:layout_width="fill_parent"></Button>
    </LinearLayout>
